{
    "base_uri": "https://api.openagenda.com/v2/",
    "endpoints": {
        "my_agenda": {
            "method": "GET",
            "path":  "me/agendas/{agendaUid}",
            "headers": {
                "accept": "application/json"
            }
        },
        "my_agendas": {
            "method": "GET",
            "path":  "me/agendas",
            "headers": {
                "accept": "application/json"
            }
        },
        "agendas": {
            "method": "GET",
            "path":  "agendas/{agendaUid}",
            "headers": {
                "accept": "application/json"
            }
        },
        "events": {
            "method": "GET",
            "path":  "agendas/{agendaUid}/events",
            "headers": {
                "accept": "application/json"
            }
        },
        "event": {
            "method": "GET",
            "path":  "agendas/{agendaUid}/events/{eventUid}",
            "headers": {
                "accept": "application/json"
            }
        },
        "locations": {
            "method": "GET",
            "path":  "agendas/{agendaUid}/locations",
            "headers": {
                "accept": "application/json"
            }
        },
        "requestAccessToken": {
            "method": "POST",
            "path":  "requestAccessToken",
            "headers": {
                "accept": "application/json"
            }
        },
        "createEvent": {
            "method": "POST",
            "path":  "agendas/{agendaUid}/events",
            "headers": {
                "accept": "application/json"
            }
        },
        "updateEvent": {
            "method": "PUT",
            "path":  "agendas/{agendaUid}/events/{eventUid}",
            "headers": {
                "accept": "application/json"
            }
        },
        "deleteEvent": {
            "method": "DELETE",
            "path":  "agendas/{agendaUid}/events/{eventUid}",
            "headers": {
                "accept": "application/json"
            }
        }
    }
}
